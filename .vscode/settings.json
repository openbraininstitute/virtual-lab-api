{
  "editor.formatOnSave": true,
  "editor.codeActionsOnSave": {
    "source.fixAll": "explicit",
    "source.organizeImports": "explicit"
  },
  "editor.defaultFormatter": "charliermarsh.ruff",
  "cSpell.words": [
    "asyncpg",
    "autoflush",
    "boto",
    "datamodels",
    "DBAPI",
    "disableable",
    "dotenv",
    "fastapi",
    "fulltext",
    "httpx",
    "ilike",
    "isnot",
    "jdbc",
    "jsonable",
    "Keycloack",
    "keycloak",
    "loguru",
    "neurosciencegraph",
    "noload",
    "onupdate",
    "poolclass",
    "pydantic",
    "regconfig",
    "sessionmaker",
    "Sparql",
    "sqlalchemy",
    "starlette",
    "totp",
    "tsvector",
    "unstar",
    "unstaring",
    "usecases"
  ],
  "search.exclude": {
    "**/.mypy_cache": true
  }
}
