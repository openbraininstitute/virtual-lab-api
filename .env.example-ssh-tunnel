# Database settings
POSTGRES_HOST=remote-db-host
POSTGRES_PORT=5432
POSTGRES_USER=db_user
POSTGRES_PASSWORD=db_password
POSTGRES_DB=db_name

# SSH Tunnel settings
USE_SSH_TUNNEL=true
SSH_HOST=ssh-bastion.example.com
SSH_PORT=22
SSH_USERNAME=ssh_user
SSH_PRIVATE_KEY_PATH=/path/to/private/key
# Optional, only if your private key is password protected
SSH_PRIVATE_KEY_PASSWORD=key_password 